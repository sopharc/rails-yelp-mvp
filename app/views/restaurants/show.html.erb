<div class = "header">
    <h1><%= @restaurant.name %></h1>
   </div>

<div>
  <ul>
    <% @restaurant.reviews.each do |review| %>
      <li>
        <%= review.rating %> -
        <%= review.content %>
      </li>
    <% end %>
  </ul>
</div>

  <li><%= link_to "Add a review for this restaurant", new_restaurant_review_path(@restaurant) %></li>
